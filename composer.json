{
    "name": "northern-estate-agencies/transporter-foundations",
    "type": "library",
    "require": {
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.0",
        "juststeveking/laravel-transporter": "^2.0",
        "guzzlehttp/guzzle": "^7.7"
    },
    "require-dev": {
        "laravel/sanctum": "^3.0",
        "laravel/pint": "^1.6",
        "pestphp/pest": "^2.6",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "TransporterFoundations\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
